{
  "name": "meli",
  "packageManager": "yarn@3.6.4",
  "version": "1.0.0",
  "description": "prueba meli",
  "main": "index.js",
  "author": "wizanchez <wianchez@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:css": "sass src/client/styles/scss/index.scss dist/styles/main.css",
    "build-dev2": "NODE_ENV=development npm run build-client && NODE_ENV=development npm run build-server",
    "build": "NODE_ENV=production npm run build-client && npm run build-server",
    "build-client": "webpack --config webpack.config.client.js",
    "build-server": "webpack --config webpack.config.server.js",
    "build-dev": "NODE_ENV=development npm run build-server && node --inspect dist/index.js",
    "dev": "nodemon --ext ts --exec ts-node \"npm run build-dev\"",
    "start": "NODE_ENV=production npm run build",
    "start-1": "NODE_ENV=production npm run build && NODE_ENV=production node dist/index.js",
    "start-dev": " NODE_ENV=development npm run build-dev2 && NODE_ENV=development node dist/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "node-sass": "^9.0.0",
    "node-sass-middleware": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.0.0",
    "react-router-dom": "^6.6.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/node-sass": "^4.11.8",
    "@types/react-dom": "^19.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  }
}